source $HOME/.zprezto/runcoms/zshrc

for config_file ($HOME/.yadr/zsh/*.zsh) source $config_file

# Mise (modern version manager) is already activated below
# No need for RVM - mise handles Ruby, Node, Python, etc.

export PATH="/opt/homebrew/opt/postgresql@14/bin:$PATH"
export LDFLAGS="-L/opt/homebrew/opt/postgresql@14/lib"
export CPPFLAGS="-I/opt/homebrew/opt/postgresql@14/include"

# Activate mise (modern version manager for Ruby, Node, Python, etc.)
eval "$(mise activate zsh)"